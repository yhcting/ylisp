;************************************************
; CHECK SYNTAX
;************************************************

MT OK
FAIL
((help 'car)

=================================================

; wrong usage of escape character.
MT OK
FAIL
(help '\y)

=================================================

; wrong usage of escape character.
MT OK
FAIL
(('a))

=================================================

; function name or (lambda ...) or (mlambda ...)
MT OK
FAIL
((eval 'help) 'ttt)
